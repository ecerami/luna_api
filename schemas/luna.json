{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "bucket": {
        "type": "object",
        "properties": {
          "file_name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "genes": {
            "type": "array",
            "items": [
              {
                "type": "string"
              }
            ]
          }
        },
        "required": [
          "file_name",
          "description",
          "url"
        ]
      }
    },
    "required": [
      "bucket"
    ]
  }